{
  "name": "@bchouse/site",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-p build:*",
    "dev": "run-p dev:*",
    "start": "cross-env NODE_ENV=production node -r dotenv/config ./build/server.js  dotenv_config_path=../../_development.env",
    "clean": "rimraf build public/build .cache",
    "postinstall": "is-docker && echo 'Skipping post-install' || run-p postinstall:*",
    "postinstall:patch": "patch-package --patch-dir ../../patches",
    "postinstall:routes": "remix-routes",
    "icons": "npx @svgr/cli --silent --out-dir app/components/icons -- public/assets/img",
    "icons:watch": "npm-watch icons",
    "dev:routes": "remix-routes -w",
    "dev:remix": "remix dev -c \"run-p remix:context remix:server\" --manual --port 4000",
    "dev:cashscript": "foreach -g **/*.cash -x \"cashc #{path} --output #{dir}/contract.json\"",
    "dev:xstate": "xstate typegen 'app/components/machines/**/*.ts?(x)' --watch",
    "test": "vitest --coverage",
    "build:routes": "remix-routes",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=esm ./server/server.ts ./server/getContext.ts --outdir=./build/ --bundle --sourcemap --packages=external  --preserve-symlinks ",
    "build:cashscript": "foreach -g **/*.cash -x \"cashc #{path} --output #{dir}/contract.json\"",
    "remix:context": "esbuild --platform=node --format=esm ./server/getContext.ts --outfile=./build/getContext.js --bundle --sourcemap --packages=external --watch",
    "remix:server": "tsx watch --ignore=./build --clear-screen=false -r tsconfig-paths/register -r dotenv/config ./server/server.ts dotenv_config_path=../../_development.env",
    "docker:build": "docker compose --env-file ../../_development.env build bchouse",
    "docker:up": "docker compose --env-file ../../_development.env up bchouse --force-recreate",
    "env:dev": "dotenv -e ../../_development.env --",
    "env:staging": "dotenv -e ../../_staging.env --"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
